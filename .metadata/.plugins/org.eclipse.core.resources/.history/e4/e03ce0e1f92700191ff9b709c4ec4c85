package controller;

import java.io.File;
import java.io.IOException;

/**
 * Sets up the bash script for you.
 * 
 * @author dpgra
 *
 */
public class Installer {

	public static void main(String[] args) throws IOException {
		File file = new File("/.bash_profile");
		if(!file.exists()) {
			file.createNewFile();
		}
		
	}
}
